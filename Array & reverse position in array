#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin >> n;
    
    int arr[n-1];
  //Nhập mảng 
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }
  //đảo ngược vị trí mảng
    for(int i = n - 1; i >= 0; i--){
        cout << arr[i] << " ";
    }
    return 0;
}
